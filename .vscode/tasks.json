{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: gcc build active file",
            "command": "/usr/bin/gcc",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "libmlx.dylib",
                "-L./libft",
                "-lft",
                "-L./gnl",
                "-lgnl",
                "-L.",
                "-lmlx",
                "-L./ft_printf",
                "-lftprintf",
                "-framework", "OpenGL",
                "-framework", "Appkit",
                "${file}",
                "-o",
                "${fileDirname}/${fileBasenameNoExtension}"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}